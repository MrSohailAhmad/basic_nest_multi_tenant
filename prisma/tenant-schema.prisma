// tenant.schema.prisma

generator client {
    provider = "prisma-client-js"
    output   = "./generated/tenant-client" // ðŸ‘ˆ unique output folder
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL") // ðŸ‘ˆ dynamically change this later
}

model TenantUser {
    id       Int      @id @default(autoincrement())
    name     String
    email    String   @unique
    tenantId String   @unique
    password String
    roleId   Int
    deleted  Boolean?
}
